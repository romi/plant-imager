from lettucethink.sony import Camera

camera_args = {
                "device_ip" : "192.168.100.101",
                "api_port" : "10000",
                "use_adb" : False,
                "use_flashair" : True,
                "flashair_host" : "192.168.100.102",
                "timeout" : 10
                }

cam=Camera(**camera_args)
cam.start()
cam.grab()
im=cam.get_data()
